@model Sklep.WebApp.Models.DTO.OrderDTO
@{
    ViewBag.Title = "CheckOut";
}

<h2>CheckOut</h2>
<script>
    $(document).ready(function () {
        var o = { Prize:"111",ItemID:"2",Name:"Nazwa",Category:"Rekawice",Quanity:"2" };
        $.ajax({
            type: "POST",
            url: "http://localhost:3060/CartService.svc/Carts/Add",
            data: JSON.stringify(o),
            contentType: "application/json",
            dataType: "json",
            processData: true,
            success: function () {
                alert("success...");
            },
            error: function () {
                alert("not working");
            }
        });
    });
</script>
  
